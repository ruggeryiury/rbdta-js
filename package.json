{
  "name": "rbdta-js",
  "version": "1.0.4",
  "private": true,
  "description": "A Rock Band song metadata file parser written in Javascript.",
  "homepage": "https://github.com/ruggeryiury/rbdta-js#readme",
  "repository": "github:ruggeryiury/rbdta-js",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "parser",
    "rock-band",
    "dta",
    "dta-file",
    "javascript",
    "typescript"
  ],
  "author": "Ruggery Iury CorrÃªa <ruggeryiury@gmail.com>",
  "license": "ISC",
  "scripts": {
    "build": "pnpm run clear && tsc --project ./prod.tsconfig.json",
    "clear": "rimraf dist",
    "dev": "tsx watch \"./tests/exec.ts\"",
    "format": "prettier --write src",
    "gen:src_exports": "python main_exports_creator.py",
    "lint": "eslint ."
  },
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./core": {
      "import": "./dist/core.js"
    },
    "./errors": {
      "import": "./dist/errors.js"
    },
    "./lib": {
      "import": "./dist/lib.js"
    }
  },
  "dependencies": {
    "jschardet": "^3.1.4",
    "path-js": "file:./packages/path-js-1.6.1.tgz",
    "set-default-options": "file:./packages/set-default-options-1.0.0.tgz"
  },
  "devDependencies": {
    "@types/node": "^22.9.1",
    "eslint": "^9.15.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.5.0",
    "eslint-plugin-n": "^17.13.2",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "type-fest": "^4.27.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0"
  }
}
